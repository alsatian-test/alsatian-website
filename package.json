{
  "name": "alsatian-website",
  "version": "1.0.0",
  "description": "Website for alsatian test framework (https://github.com/alsatian-test/alsatian)",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-typescript && npm run build-sass",
    "build-sass": "node-sass --include-path scss public/sass/styles.scss public/css/styles.css",
    "build-typescript": "tsc",
    "dev": "npm-run-all --parallel start expose-to-networked-devices",
    "expose-to-networked-devices": "browser-sync start --proxy localhost:2900 --files \"css/*.css\"",
    "review": "npm run review-typescript",
    "review-typescript": "tslint \"**/*.ts\" \"**/*.tsx\" --exclude \"**/*.d.ts\" --exclude \"node_modules/**/*.*\"",
    "start": "node ./index.js",
    "pretest": "npm run build",
    "test": "npm run review && npm run unit-tests",
    "unit-tests": "alsatian \"./public/**/*.spec.js\" \"./server/**/*.spec.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alsatian-test/alsatian-website.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alsatian-test/alsatian-website/issues"
  },
  "homepage": "https://github.com/alsatian-test/alsatian-website#readme",
  "dependencies": {
    "alsatian": "^1.0.0-beta.2",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "highlight.js": "^9.8.0",
    "monaco-editor": "^0.5.1",
    "prismjs": "^1.5.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-router": "^2.8.1",
    "redux": "^3.5.2",
    "reflect-metadata": "^0.1.3",
    "systemjs": "^0.19.31",
    "typescript": "^2.0.8"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/express": "^4.0.33",
    "@types/history": "^2.0.39",
    "@types/prismjs": "^1.4.18",
    "@types/react": "^0.14.46",
    "@types/react-dom": "^0.14.18",
    "@types/react-router": "^2.0.38",
    "breakpoint-sass": "^2.7.0",
    "browser-sync": "^2.17.5",
    "node-sass": "^3.10.1",
    "npm-run-all": "^3.1.1",
    "susy": "^2.2.12",
    "tslint": "^4.0.2"
  }
}
